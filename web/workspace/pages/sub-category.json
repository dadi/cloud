{
  "page": {
    "key": "sub-category"
  },
  "settings": {
    "passFilters": true
  },
  "template": "category.dust",
  "datasources": [
    "langs",
    "translations",
    "pages",
    "web-services",
    "sub-categories",
    "category-articles"
  ],
  "requiredDatasources": [
    "sub-categories"
  ],
  "events": [
    "i18n",
    "metatags"
  ],
  "routes": [
    {
      "path": "/:lang/:category/:subCategory",
      "params": [
        {
          "param": "subCategory",
          "fetch": "sub-categories"
        }
      ]
    }
  ]
}